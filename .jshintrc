{
    "node": true,
    "browser": true,
    "es5": false,
    "esnext": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "smarttabs": true,
    "strict": true, 
    "predef": [
        "inject",
        "describe",
        "describe",
        "it",
        "xit",
        "beforeEach",
        "afterEach",
        "expect",
        "spyOn",
        "runs",
        "waits",
        "waitsFor",
        "jasmine"
    ],
    "globals": {
        "angular": false
    }
}
